{
  "name": "flow-typed",
  "description": "A repository of high quality flow type definitions",
  "license": "MIT",
  "homepage": "https://github.com/flow-typed/flow-typed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flow-typed/flow-typed.git"
  },
  "bugs": {
    "url": "https://github.com/flow-typed/flow-typed/issues"
  },
  "version": "3.3.1",
  "main": "dist/cli.js",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "mkdirp dist && babel ./src --out-dir=./dist",
    "clean": "rimraf dist",
    "flow": "flow",
    "lint": "eslint .",
    "prepublish": "mkdirp dist && yarn test",
    "test": "yarn clean && yarn build && yarn test-quick",
    "test-quick": "jest && yarn lint",
    "watch": "mkdirp dist && babel --source-maps --watch=./src --out-dir=./dist"
  },
  "dependencies": {
    "@octokit/rest": "^16.43.1",
    "colors": "^1.4.0",
    "flowgen": "^1.10.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "got": "^10.5.7",
    "md5": "^2.2.1",
    "mkdirp": "^1.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semver": "7.3.2",
    "table": "^5.4.6",
    "through": "^2.3.8",
    "unzipper": "^0.10.8",
    "which": "^2.0.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "flow-bin": "^0.144.0",
    "jest": "^25.1.0"
  },
  "keywords": [
    "flow",
    "flowtype"
  ],
  "jest": {
    "name": "flow-typed-cli",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/.*",
      "<rootDir>/node_modules/.*"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.*/__[^/]*-fixtures__/.*"
    ]
  }
}
