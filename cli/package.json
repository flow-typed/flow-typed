{
  "name": "flow-typed",
  "description": "A repository of high quality flow type definitions",
  "license": "MIT",
  "homepage": "https://github.com/flow-typed/flow-typed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flow-typed/flow-typed.git"
  },
  "bugs": {
    "url": "https://github.com/flow-typed/flow-typed/issues"
  },
  "version": "2.6.2",
  "main": "dist/cli.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "mkdirp dist && babel ./src --out-dir=./dist",
    "clean": "rimraf dist",
    "flow": "flow",
    "lint": "eslint .; exit 0",
    "prepublish": "mkdirp dist && yarn test",
    "test": "yarn clean && yarn build && yarn test-quick",
    "test-quick": "jest && yarn lint",
    "watch": "mkdirp dist && babel --source-maps --watch=./src --out-dir=./dist"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@octokit/rest": "^16.33.1",
    "colors": "^1.3.2",
    "flowgen": "^1.9.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "got": "^8.3.2",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "semver": "^5.5.1",
    "table": "^5.0.2",
    "through": "^2.3.8",
    "unzipper": "^0.9.3",
    "which": "^1.3.1",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-prettier": "^2.7.0",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0"
  },
  "keywords": [
    "flow",
    "flowtype"
  ],
  "jest": {
    "name": "flow-typed-cli",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/.*",
      "<rootDir>/node_modules/.*"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.*/__[^/]*-fixtures__/.*"
    ]
  }
}
