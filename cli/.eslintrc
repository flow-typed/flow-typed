{
  "parser": "babel-eslint",
  "env": { 
    "es6": true
  },
  "plugins": [
    "flow-vars"
  ],
  "globals": {
  },
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "eqeqeq": [2, "smart"],
    "semi": 2,
    "no-unused-vars": ["error", {
      "varsIgnorePattern": "^_.?.?$",
      "argsIgnorePattern": "^_.*$",
    }],
  }
}
